<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= color %> player</title>
    
    <link rel="stylesheet" type="text/css" href="/css/styles.css">
   
</head>
<body>
  
   <nav class="navbar">
      <div class="logo">Your Logo</div>
      <div class="info-container">
        <p id="info">Player: <span id="player"></span></p>
        <div id="timer"></div>
        <div id="withdrawal">withdrawal</div>
      </div>
      <ul class="nav-links">
          <li><a href="#">Home</a></li>
          <li><a href="#">About</a></li>
          <li><a href="#">Services</a></li>
          <li><a href="#">Contact</a></li>
      </ul>
    </nav>

    <p id="info-display"></p>

    <div id="frame">
        <div id="up">A B C D E F G H</div>
        <div id="down">A B C D E F G H</div>
        <div id="left">
          <div class="row-label">8</div>
          <div class="row-label">7</div>
          <div class="row-label">6</div>
          <div class="row-label">5</div>
          <div class="row-label">4</div>
          <div class="row-label">3</div>
          <div class="row-label">2</div>
          <div class="row-label">1</div>
        </div>
        <div id="right">
          <div class="row-label">8</div>
          <div class="row-label">7</div>
          <div class="row-label">6</div>
          <div class="row-label">5</div>
          <div class="row-label">4</div>
          <div class="row-label">3</div>
          <div class="row-label">2</div>
          <div class="row-label">1</div>
        </div>
    
        <div id="gameboard" class="<%= color %>"></div>
    </div>
    
    <div class="taken-pieces-board white"></div>
    <div class="taken-pieces-board black"></div>
    
    <script src="/socket.io/socket.io.js"></script>
    <script type="module" src="/js/app.js"></script>

</body>
</html>